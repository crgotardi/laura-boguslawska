---
import { Image } from 'astro:assets';

interface Props {
    src: ImageMetadata;
    alt: string;
    width: number;
    height: number;
}

const { src, alt, width, height } = Astro.props;
---

<Image 
    src={src}
    alt={alt}
    width={width}
    height={height}
    class="rounded-full"
/>
